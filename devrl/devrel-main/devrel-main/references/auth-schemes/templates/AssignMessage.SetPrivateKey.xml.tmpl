<AssignMessage name="AssignMessage.SetPrivateKey">
    <AssignVariable>
        <Name>private.jwt.key</Name>
        <!--
         This private key is not and must not be used elsewhere.
         The key is only for the purposes of this reference
         implementation. An appropriate approach would be to
         persist this key in an Apigee Key Value Map and retrieve
         the key at runtime.
        -->
        <Value>
$_JWT_PRIVATE_KEY
        </Value>
    </AssignVariable>
</AssignMessage>